{
  "name": "OpenPIMS",
  "description": "No more cookie banner",
  "version": "1.1.0",
  "author": "Stefan BÃ¶ck",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "128": "openpims.png"
  },
  "action": {
    "default_icon": "openpims.png",
    "default_popup": "action.html",
    "default_title": "OpenPIMS"
  },
  "permissions": [
    "declarativeNetRequest",
    "webNavigation",
    "storage",
    "tabs"
  ],
  "host_permissions": [
    "<all_urls>"
  ]
}